{
  "name": "@ztrehagem/woodpecker",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.8.1",
  "scripts": {
    "prepare": "pnpm run types",
    "types": "wrangler types",
    "build": "tsc -b && vite build",
    "preview": "npm run build && vite preview",
    "dev": "vite",
    "lint": "pnpm run --aggregate-output '/^(eslint|prettier|typecheck\\..+)$/'",
    "fix": "pnpm run eslint:fix && pnpm run prettier:fix",
    "eslint": "eslint .",
    "eslint:fix": "pnpm run eslint --fix",
    "prettier": "prettier --ignore-path .gitignore --ignore-path .prettierignore --check .",
    "prettier:fix": "pnpm run prettier --write",
    "typecheck": "pnpm run --aggregate-output '/^typecheck\\./'",
    "typecheck.app": "tsc --noEmit -P tsconfig.app.json",
    "typecheck.node": "tsc --noEmit -P tsconfig.node.json",
    "typecheck.worker": "tsc --noEmit -P tsconfig.worker.json",
    "test": "false"
  },
  "dependencies": {
    "hono": "4.7.5",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.0.2",
    "@eslint/js": "9.23.0",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.3.4",
    "eslint": "9.23.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "globals": "15.15.0",
    "prettier": "^3.5.3",
    "typescript": "5.8.2",
    "typescript-eslint": "8.29.0",
    "vite": "6.2.5",
    "wrangler": "4.12.0"
  }
}
